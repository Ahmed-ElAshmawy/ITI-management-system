@model List<ITI_System.Models.Student>
@{
    var Dep = TempData["deptartment"] as Department;
}
<div class="table-responsive">
    <table class="table table-bordered table-hover">
        <tr>
            <th>
                Department ID
            </th>
            <th>
                Department Name
            </th>
            <th>
                Department Capacity
            </th>
            <th>
                Manager ID
            </th>
        </tr>
            <tr>
                <td>
                   
                    @Dep.Dept_Id
                </td>
                <td>
                    @Dep.Dept_name
                </td>
                <td>
                    @Dep.Dept_Capacity
                </td>
                <td>
                    @Dep.Manager_Id
                </td>
               
            </tr>
    </table>
    <table class="table table-bordered table-hover">
        <tr>
            
            <th>
                Student UserName
            </th>
            <th>
                Student FirstName
            </th>
            <th>
                Student LastName
            </th>
            <th>
                Student BirthDate
            </th>
            <th>
                Student Email
            </th>
            <th>
                Student Married
            </th>
            <th>
                Student PhoneNumber
            </th>
            <th>
                Student Attendence
            </th>
        </tr>
        @foreach (var item in Model)
            {
            <tr>
               
                <td>
                    @Html.DisplayFor(a=>item.UserName)
                </td>               
                <td>
                    @Html.DisplayFor(a => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(a => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(a => item.BirthDate)
                </td>
                <td>
                    @Html.DisplayFor(a => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(a => item.IsMarried)
                </td>
                <td>
                    @Html.DisplayFor(a => item.PhoneNumber)
                </td>
                <td>
                    @Html.DisplayFor(a => item.Std_Attendence_Grade)
                </td>
        </tr>
        }
    </table>
</div>