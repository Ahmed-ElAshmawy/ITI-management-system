@{
    ViewBag.Title = "Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html>
<head>
    <meta />
    <title>Import Department</title>
</head>
<body>
    @using (Html.BeginForm("Import", "Department", FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
    {
        
        <h2>Excel File</h2>
        <input type="file" name="excelFile" />
        <br />
        <input type="submit" value="Import" />
        <p style="font-size:20px">@Html.Raw(ViewBag.Error)</p>
    }
    @Html.ActionLink("Back to List", "Index", "Department", new { @class = "btn btn-warning" })
</body>
</html>
